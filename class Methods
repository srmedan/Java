
public class Methods {
	public static void main(String[] args) {
		sayHelloWorld();

		sayHelloTo("charlie");
		sayHelloTo("John");
		
		int x = returnFive();
		
		System.out.println(x);
		
		int result = square(returnFive());
		
		System.out.println(result);
		
		
	}
	// this method both returns a value and takes in data
		static int square(int x) {
			return x * x;
		}
	
	
	// this method return an int type with the value 5
	static int returnFive () {
		return 5;
	}
		
	
	//this method will say hello to whatever name is passed when called
	static void sayHelloTo(String name) {
		System.out.println("Hello, " + name);
		
		
		
	}
	// this method will say hello world
	static void sayHelloWorld() {
		System.out.println("Hello, world!");
	}
	
	
}
