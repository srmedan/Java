package tutorials.basic;

public class AccessModifiers {
	
	private int x;
	public int y;
	
	
	
	public static void main(String[] args) {
		
	}
	
	// public visibility means that this method can be accessd anywhere in the program so 
	//long as you gave an instance of this class to call it from
	
	public void doSomethingPublic() {
		
	}
	
	
	// private visibility means that this method can't be accessed anywhere other than inside of this class
	
	private void doSomethingPrivate() {
		
	}
	
	
	// protected visibility means that this mehtod can only be accessed inside of this package
	// and from subclasses of this class
	
	protected void doSomethingProtected() {
		
	}
	
	void doSomeThing() {
		
	}


}
